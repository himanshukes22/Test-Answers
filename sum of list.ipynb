     "text": [
      "-5\n"
     ]
    }
   ],
   "source": [
    "def sum_list(a):\n",
    "    s=0\n",
    "    for i in a:\n",
    "        s=s+i \n",
    "    return s\n",
    "print(sum_list([1,2,-8]))"
